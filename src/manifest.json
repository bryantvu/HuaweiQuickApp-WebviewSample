{
    "package": "com.bvutest.myquickapp",
    "name": "My Quick App",
    "versionName": "1.0.0",
    "versionCode": 1,
    "icon": "/Common/logo.png",
    "minPlatformVersion": 1050,
    "features": [
        {
            "name": "system.webview"
        },
        {
            "name": "system"
        }
    ],
    "permissions": [
        {
